#include<string.h>
#include <stdio.h>
int a [6]; b [6]; c [6];

int main()
{   
    for(int i=0; i<6; i++)
        {printf("ingrese los 6 valores de A: ");
        scanf("%i", &a[i]);}
    for(int i=0; i<6; i++)
        {printf("ingrese los 6 valores de B: ");
        scanf("%i", &b[i]);}
    for (int i=0; i<6;i++)
        {c[i]= a[i]+ b[i];
        printf("el resultado es %i\n", c[i]);
        }
    
    return 0;
}
